<a class="block-link chart-cell-content" href="{{ctx}}/client/{{client_id}}#{{name}}">

    <label class='chart-cell-label' data-value="{{initial_provider}}" for="initial_provider-filter">{{initial_provider_name}}</label>
    <label class='chart-cell-label' data-value="{{current_provider}}" for="current_provider-filter">{{current_provider_name}}</label>
    <label class='chart-cell-label' data-value="{{code}}" for="code-filter">{{code}}</label>
    <label class='chart-cell-label' data-value="{{scale}}" for="scale-filter">{{scale}}</label>
    <label class='chart-cell-label' data-value="{{freeword}}" for="freeword-filter">{{freeword}}</label>
    <label class='chart-cell-label' data-value="{{start_at}}" for="start_at-filter">{{start_at}}</label>

    <div class="chart-cell-color-mark"></div>
    <div class="chart-cell-color-mark-hover"></div>
</a>